import java.io.*;
import java.util.Scanner;

class bruhtest {
    public static void main(String args[]) throws IOException {
        Scanner in = new Scanner(System.in);

        int a = 0;
        int b = 0;
        int space = 0;
        char mycharA = 0;
        char mycharB = 0;


        System.out.println("Yo");
        a = System.in.read();
        System.out.println(a);

       if (a > 33){ 
        a = System.in.read();
        System.out.println(a);
        b = System.in.read();
        System.out.println(b);
       }
        System.out.println(a);
        System.out.println(b);
        mycharA = (char) a;
        mycharB = (char) b;

        System.out.println(a);
        System.out.println(b);
        System.out.print(mycharA);
        System.out.print(mycharB);
    }
}
